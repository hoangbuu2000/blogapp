@model IEnumerable<BlogApp.Areas.Admin.Data.Log>

@{
    ViewBag.Title = "Lịch sử hoạt động";
}

<h2>Lịch sử hoạt động</h2>

<table class="table">
    <tr>
        <th>
            Nội dung
        </th>
        <th>
            Ngày thực hiện
        </th>
        <th>
            Người thực hiện
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Content)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PubDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Account.Username)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.ID }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
